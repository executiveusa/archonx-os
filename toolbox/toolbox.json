{
  "name": "ArchonX Toolbox",
  "version": "1.1.0",
  "skills": [
    "toolbox/skills/remotion_video_generator.json",
    "toolbox/skills/access_grant_orchestrator.json",
    "toolbox/skills/pauliwheel_coding_protocol.skill.json",
    "toolbox/skills/agent_lightning_bootstrap.skill.json",
    "toolbox/skills/beads_manager.skill.json",
    "toolbox/skills/ralphy_loop_enforcer.skill.json"
  ],
  "tools": [
    "toolbox/tools/request_grant.py",
    "toolbox/tools/audit_emit.py"
  ],
  "workflows": [
    "toolbox/workflows/grant-request-approval-playbook.json",
    "toolbox/workflows/restricted-mode-recovery-playbook.json"
  ],
  "security": {
    "requires_grant": true,
    "requires_work_item": true
  },
  "endpoints": {
    "remotion_render_url": "${ARCHONX_REMOTION_RENDER_URL:-http://localhost:7555/v1/render}"
  },
  "defaults": {
    "coding_behavior": "pauliwheel_coding_protocol",
    "requires_bead_id_for_code_actions": true,
    "enforce_agent_lightning_first": true
  }
}
